{
  "manifest_version": 3,
  "name": "Identity Protection Agent",
  "version": "1.0",
  "description": "Identity protection!",
  "action": {
    "default_popup": "Interface.html",
    "default_icon": {
      "16": "Icon.png",
      "48": "Icon.png",
      "128": "Icon.png"
    }
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
